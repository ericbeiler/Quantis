{
  "version": "2.0",
  "functionTimeout": "00:10:00",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Request,Dependency,Metric"
      },
      "enableLiveMetricsFilters": false
    }
  },
  "extensions": {
    "blobs": {
      "maxDegreeOfParrallelism": 1,
      "poisonBlobThreshold": 10
    },
    "queues": {
      "maxDequeueCount": 15,
      "batchSize": 1
    }
  }
}